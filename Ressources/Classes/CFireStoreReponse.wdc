         	SI UnMembreDocument..Membre..Occurrence = 0 ALORS
         		SI UnMembreDocument..Nom = "name" ALORS				
         			{"m_jsonDocumentTemp.id", indVariable} = ExtraitChaîne(UnMembreDocument..Valeur, 7, "/")			
